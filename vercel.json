{
  "version": 2,
  "buildCommand": "echo 'No build step needed'",
  "outputDirectory": "./",
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/duralux-admin/(.*)",
      "dest": "/duralux-admin/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ]
}